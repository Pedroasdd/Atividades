//Faça uma função capaz de receber dois números b e h que representam os lados de um
//retângulo ou quadrado, e retornar 1 se ele for um quadrado ou 0 se for um retângulo.

#include<stdio.h>

int base(int b, int h){

    if(b==h){
        return 1;
    }
    else{
        return 0;
    }

}

int main(){

    int b,h;

    scanf("%d %d", &b, &h);

    int resultado = base(b,h);

    if(resultado == 1){
        printf("quadrado\n");
    }
    else
        printf("triangulo\n");


    return 0;
}
